<p style="color: green"><%= notice %></p>

<h1>Members</h1>

<div id="members" cy-testId="all-members">
  <% @members.each do |member| %>
    <%= render member %>
    <p>
      <%= link_to "Show this member", member, "cy-testId": "show-senior#{member.id}"%>
    </p>
  <% end %>
</div>

<%= link_to "New member", new_member_path, "cy-testId": "new-member-button" %>
